nav {
  background: #F3F3F3;
  display: block;
  height: 100%;
  overflow: auto;
  position: fixed;
  font-size: 14px;
  top: 0;
  width: 24em;
  transition: top 0.3s ease-in-out 0s;
  -webkit-transition: top 0.3s ease-in-out 0s;
  -moz-transition: top 0.3s ease-in-out 0s;
  -o-transition: top 0.3s ease-in-out 0s;
}

.main-menu li {
  font-weight: bold;
  text-transform: uppercase;
  font-size: 15px;
  border-bottom: 1px solid #DAD6D2;
  background-color: #F3F3F3;
}

.main-menu li a {
  color: black;
  text-decoration: none;
  display: block;
  top: 0px;
  right: 0px;
  width: 100%;
  height: 100%;
  padding: 12px 12px 12px 20px;
}

.main-menu li:not(.nav-close) a:hover {
  background-color: white;
}

.main-menu li:not(.nav-close):hover {
  background-color: white;
}

.main-menu li:not(.nav-close) a:active {
  background-color: white;
}

.main-menu li:not(.nav-close):active {
  background-color: white;
}

.main-menu li .sub-nav {
  border-bottom: 0px;
  padding: 4px;
}

.main-menu li.sub-nav {
  font-weight: normal;
  text-transform: capitalize;
  font-size: 15px;
  padding-left: 20px;
}

.icon:before {
  font-family: 'FontAwesome';
  font-style: normal;
  font-variant: normal;
  font-weight: normal;
  content: '\f105';
  line-height: 1;
  text-transform: none;
  margin: 0.1em 0.8em -0.1em 0;
  opacity: 1;
  -webkit-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  transition: all 0.2s ease-in-out;
}

.icon-open:before {
  content: '\f106';
  -webkit-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  transition: all 0.2s ease-in-out;
}

.icon-closed:before {
  content: '\f105';
  -webkit-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  transition: all 0.2s ease-in-out;
}

.sub-nav a::after {
  float: right;
  content: url("../../images_/menu-check-gray.svg");
  width: 19px;
  height: 19px;
  vertical-align: middle;
}

.sub-nav.complete a::after {
  content: url("../../images_/menu-check-green.svg");
}

.sub-nav.inprogress a::after {
  content: url("../../images_/menu-check-yellow.svg");
}
