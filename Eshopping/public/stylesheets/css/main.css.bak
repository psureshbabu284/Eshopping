  height: 100%;
  z-index: 999;
  display: none;
  transition: top 0.4s ease-in-out 0s;
  -webkit-transition: top 0.4s ease-in-out 0s;
  -moz-transition: top 0.4s ease-in-out 0s;
  -o-transition: top 0.4s ease-in-out 0s;
}

