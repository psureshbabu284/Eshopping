/* HEADER STYLES
-------------------------------------------------- */
.navbar-dark {
  background-color: #1E1E1E;
  border-color: #1E1E1E;
  height: 30px;
}

.navbar-white {
  background-color: white;
  height: 35px;
  -webkit-box-shadow: 0 4px 3px -2px #DAD6D2;
  -moz-box-shadow: 0 4px 3px -2px #DAD6D2;
  box-shadow: 0 4px 3px -2px #DAD6D2;
}

.login-holder {
  background-color: #397664;
  width: 100%;
  min-width: 200px;
  height: 30px;
  color: white;
  font-size: 12px;
  text-align: center;
  line-height: 30px;
  vertical-align: middle;
  padding-left: 20px;
  padding-right: 20px;
}

.login-holder:hover {
  background-color: #2B6D5E;
}

span.login-icon:before {
  content: url("../../images_/user-icon.svg");
  background-repeat: no-repeat;
  margin-right: 10px;
  width: 15px;
  height: 18px;
  line-height: 30px;
  vertical-align: middle;
}

#navbar-inter-2 {
  top: 30px;
}

.navbar-brand {
  float: left;
  padding: 5px;
}

.container > .navbar-header {
  display: inline-block;
}

.progress-label {
  display: none;
}

#progress-bar {
  width: 100%;
  height: auto;
  display: inline;
  padding-left: 2%;
  padding-right: 3%;
}

.nav-expander .nav-open {
  color: #397664;
  padding: 30px 0;
  width: 65px;
  height: 55px;
  display: inline;
}

.progress {
  background-color: #DAD6D2;
  margin-top: 1%;
}

.navbar-brand img {
  width: 36px;
  height: auto;
  margin-top: 10px;
}

.nav-expander {
  margin-top: 0;
}

@media (min-width: 768px) {
  #content {
    margin-top: 125px;
    background-color: #F3F3F3;
    min-height: 100%;
    padding-top: 40px;
    padding-bottom: 40px;
  }
  .navbar-brand {
    padding: 15px;
  }
  .navbar-dark {
    height: 48px;
  }
  .login-holder {
    height: 48px;
    font-size: 14px;
    line-height: 50px;
  }
  span.login-icon:before {
    line-height: 48px;
  }
  .navbar-white {
    height: 92px;
    -webkit-box-shadow: 0 8px 6px -3px #DAD6D2;
    -moz-box-shadow: 0 8px 6px -3px #DAD6D2;
    box-shadow: 0 8px 6px -3px #DAD6D2;
  }
  #navbar-inter-2 {
    top: 48px;
  }
  .navbar-brand img {
    width: 92px;
    height: auto;
    margin-top: 0;
  }
  .progress-label {
    text-align: right;
    font-weight: bold;
    width: 100%;
    display: block;
  }
  .progress {
    margin-top: 1%;
  }
  .nav-expander .nav-open {
    border: 1px solid #DAD6D2;
    padding: 10px 7px;
    display: block;
  }
  .nav-expander {
    margin-top: 10px;
  }
  .hide-mobile {
    display: block;
  }
}

.progress, .progress-bar {
  height: 7px;
}

.progress-bar {
  background-color: #008dbd;
  height: 7px;
}

#landing-page #progress-bar, #account #progress-bar {
  display: none;
}

#landing-page div.dropdown, #account div.dropdown {
  display: none;
}

#landing-page .navbar-white {
  box-shadow: none;
}

#nav-close {
  color: white;
  background-color: #397664;
  font-size: 12px;
  border: 1px solid #397664;
  font-weight: bold;
  padding: 10px 7px;
  width: 65px;
  height: 55px;
  display: block;
  text-align: center;
  text-decoration: none;
  text-transform: uppercase;
  margin-right: 15px;
}

span.nav-close:before {
  content: url("../../images_/close-icon.svg");
  background-repeat: no-repeat;
  margin-right: 10px;
  width: 15px;
  height: 18px;
  vertical-align: middle;
}

a.nav-expander {
  display: block;
  font-size: 12px;
  font-weight: bold;
  text-align: center;
  text-decoration: none;
  text-transform: uppercase;
  z-index: 12;
  padding-top: 10px;
  margin-right: 15px;
}

a.nav-expander:hover {
  cursor: pointer;
}

.row.navbar-dark .container {
  padding-right: 0;
}

@media (min-width: 768px) {
  .row.navbar-dark .container {
    padding-right: 15px;
  }
  .navbar-white .container {
    padding-right: 0px;
  }
  a.nav-expander {
    padding-top: 14px;
  }
}

