form {
  margin-left: -15px;
}

.form-control {
  border: 1px solid #DAD6D2;
  width: 275px;
  min-height: 45px;
}

label.control-label {
  display: inline-block;
  cursor: pointer;
  position: relative;
  padding-left: 0;
  margin-right: 15px;
  padding-bottom: 5px;
  font-size: 16px;
  font-weight: bold;
  text-transform: uppercase;
}

input.standard-check[type=checkbox] {
  display: none;
  margin-left: 15px;
}

input.standard-check[type=checkbox] + label {
  background: url("../../images_/check-box-idle.png") no-repeat 0 0;
  background-size: 24px;
  width: 24px;
  height: 24px;
  display: inline-block;
  padding: 0 0 0 0px;
  vertical-align: middle;
  margin-left: 15px;
}

input[type=checkbox]:checked + label {
  background: url("../../images_/check-box-checked.png") no-repeat 0 0;
}

input[type=radio] {
  display: none;
  margin-left: 15px;
}

input[type=radio] + label {
  background: url("../../images_/radio-idle.png") no-repeat 0 0;
  background-size: 24px;
  width: 24px;
  height: 24px;
  display: inline-block;
  padding: 0 0 0 0px;
}

input[type=radio]:checked + label {
  background: url("../../images_/radio-checked.png") no-repeat 0 0;
  background-size: 24px;
  height: 24px;
  width: 24px;
  display: inline-block;
  padding: 0 0 0 0px;
}

.multi-select-box, .multi-select-box-gray {
  border: solid 1px #dad6d2;
  padding: 8px 0;
  margin: 5px 5px;
  width: 100%;
  background: white;
  cursor: pointer;
}

.multi-select-box-gray {
  background: #f5f5f5;
}

input.multiselect-radio[type=radio] {
  display: none;
  margin-left: 15px;
}

input.multiselect-radio[type=radio] + label, input.multiselect-check[type=checkbox] + label {
  background: url("../../images_/radio-idle.png") no-repeat 0 0;
  cursor: pointer;
  background-size: 24px;
  width: 95%;
  height: 40px;
  display: inline-block;
  padding: 0 0 0 45px;
  vertical-align: middle;
  margin: 0 0 0 15px;
  font-weight: normal !important;
}

@media (min-width: 768px) {
  input.multiselect-radio[type=radio] + label, input.multiselect-check[type=checkbox] + label {
    height: 24px;
  }
}

input.multiselect-radio[type=radio]:checked + label {
  background: url("../../images_/radio-checked.png") no-repeat 0 0;
}

input.multiselect-check[type=checkbox] {
  display: none;
  margin-left: 15px;
}

input.multiselect-check[type=checkbox] + label {
  background: url("../../images_/check-box-idle.png") no-repeat 0 0;
}

input.multiselect-check[type=checkbox]:checked + label {
  background: url("../../images_/check-box-checked.png") no-repeat 0 0;
}

.form-control {
  border-radius: 0px !important;
  -webkit-box-shadow: none !important;
  box-shadow: none !important;
  height: 40px;
}

.form-control:focus {
  border: solid 1px #30578B;
}

select#select-override {
  border-radius: 0px !important;
}

select.form-control {
  overflow: hidden;
  border: none;
  display: inline-block;
  border: 1px solid #ccc;
  border-radius: 0px !important;
}

select.form-control option {
  width: 100%;
  height: auto;
  border: 1px solid #2E2E2E;
  background-color: #F3F3F3;
  display: inline-block;
  float: left;
  padding: 10px 0px 10px 30px;
  margin-bottom: 7px;
}

select.form-control span {
  display: inline-block;
  margin-left: 10px;
}

label {
  font-weight: bold !important;
}

.checkbox-label {
  display: inline-block;
  margin-left: 20px;
  margin-bottom: 10px;
}

.form-notes {
  padding: 0;
  margin-top: 10px;
}

.controls.form-inline {
  margin-left: -15px;
}

.form-horizontal .has-feedback .form-control-feedback {
  top: 40PX;
  right: -30px;
}

.onload-hide {
  display: none;
}

@media (min-width: 768px) {
  .col-align-right {
    padding-right: 0px;
  }
  .col-align-right .multi-select-box, .col-align-right .multi-select-box-gray {
    margin: 5px 15px;
  }
  .form-horizontal .control-label {
    text-align: left;
  }
}

